{
  "id": "/redis",
  "cpus": 1,
  "mem": 128,
  "disk": 0,
  "instances": 1,
  "container": {
    "docker": {
      "image": "redis",
      "forcePullImage": false,
      "privileged": false,
      "portMappings": [
        {
          "containerPort": 6379,
          "protocol": "tcp",
          "name": "redis"
        }
      ],
      "network": "USER"
    }
  },
  "ipAddress": {
    "networkName": "dcos"
  }
}
