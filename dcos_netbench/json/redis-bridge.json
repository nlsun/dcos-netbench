{
  "id": "/redis",
  "instances": 1,
  "cpus": 1,
  "mem": 128,
  "disk": 0,
  "gpus": 0,
  "backoffSeconds": 1,
  "backoffFactor": 1.15,
  "maxLaunchDelaySeconds": 3600,
  "container": {
    "docker": {
      "image": "nlsun/redis",
      "forcePullImage": false,
      "privileged": false,
      "portMappings": [
        {
          "containerPort": 6379,
          "protocol": "tcp",
          "name": "redis"
        }
      ],
      "network": "BRIDGE"
    }
  },
  "upgradeStrategy": {
    "minimumHealthCapacity": 1,
    "maximumOverCapacity": 1
  },
  "portDefinitions": [
    {
      "port": 10000,
      "protocol": "tcp",
      "labels": {}
    }
  ],
  "requirePorts": false
}
